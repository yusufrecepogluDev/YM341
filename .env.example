# JWT Settings
JWT_SECRET_KEY=your_secret_key_minimum_32_characters_long
JWT_ISSUER=ClupApi
JWT_AUDIENCE=KampusEtkinlik
JWT_EXPIRATION_MINUTES=60

# N8n Chatbot Settings
N8N_WEBHOOK_URL=https://yusufrecepoglu.app.n8n.cloud/webhook/8ff4e3b7-e759-4193-9aed-fd3e6a397512/chat
N8N_TIMEOUT_SECONDS=30
N8N_RETRY_COUNT=2
